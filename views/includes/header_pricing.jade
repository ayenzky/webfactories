#paragraph-8591-container
    .header_bg.section
      .container
        .row
          .svg-earth-pattern
            img.animate-earth-small(src='img/slider-earth-pattern-small.png')
            img.animate-earth-large(src='img/slider-earth-pattern-large.png')
        .subtitle.col-md-12
          h1.title We enhance the online
          p.lead credibility of your business

 #myModal.modal.fade(tabindex='-1')
  .modal-dialog
    .modal-content
      .modal-header
        button.close(type='button', data-dismiss='modal') ×
        h4#myModalLabel.modal-title Webfactories - Pages Pricing
      .modal-body
        // Begin MailChimp Signup Form
        #mc_embed_signup
          form#mc-embedded-subscribe-form-2.validate(action='//webfactories.us10.list-manage.com/subscribe/post?u=d60988bc77e93d3ea0ddf6358&id=e4c0cc6be4', method='post', name='mc-embedded-subscribe-form', novalidate='', target='_blank')
            #mc_embed_signup_scroll
              .indicates-required
                span.asterisk *
                |  indicates required
              .mc-field-group
                label(for='mce-FNAME')
                  | Full Name:
                  span.asterisk *
                input#mce-FNAME.required(name='FNAME', type='text', value='')
              .mc-field-group
                label(for='mce-EMAIL')
                  | Email Address:
                  span.asterisk *
                input#mce-EMAIL.required.email(name='EMAIL', type='email', value='')
              #mce-responses.clear  
              // real people should not fill this in and expect good things - do not remove this or risk form bot signups
              div(style='position: absolute; left: -5000px;')
                input(tabindex='-1', name='b_d60988bc77e93d3ea0ddf6358_e4c0cc6be4', type='text', value='')
              .clear
                input#mc-embedded-subscribe.button(name='subscribe', type='submit', value='Get A Quote')

